                    minutes2[<%-JSON.stringify(product_bottom[i].ID)%>] = Math.floor((distance2[<%-JSON.stringify(product_bottom[i].ID)%>] % (1000 * 60 * 60)) / (1000 * 60));
                    seconds2[<%-JSON.stringify(product_bottom[i].ID)%>] = Math.floor((distance2[<%-JSON.stringify(product_bottom[i].ID)%>] % (1000 * 60)) / 1000);
