<body>
<%- include('header.ejs')%>
<!--============= Hero Section Starts Here =============-->
<div class="hero-section style-2">
    <div class="container">
        <ul class="breadcrumb">
            <li>
                <a href="index.html">Home</a>
            </li>
            <li>
                <a href="#0">Pages</a>
            </li>
            <li>
                <span>Vehicles</span>
            </li>
        </ul>
    </div>
    <div class="bg_img hero-bg bottom_center" data-background="assets/images/banner/hero-bg.png" style="background-image: url(&quot;./assets/images/banner/hero-bg.png&quot;);"></div>
</div>
<!--============= Hero Section Ends Here =============-->
<!--============= Featured Auction Section Starts Here =============-->
<section class="featured-auction-section padding-bottom mt--240 mt-lg--440 pos-rel">
    <div class="container">
        <div class="section-header cl-white mw-100 left-style">
            <h3 class="title">Auction is about to end !</h3>
        </div>
        <div class="row justify-content-start mb-30-none">
            <script type="text/javascript">
                var tEnd=[],counDownDate=[],ID=[],x=[],now=[],distance=[],days=[],hours=[],minutes=[],seconds=[];
            </script>
            <%for(var i=0;i<product_top3.length;i++){ %>
            <div class="col-sm-10 col-md-6 col-lg-4 " id="<%=product_top3[i].ID%>" >
                <div class="auction-item-2">
                    <div class="auction-thumb">
                        <a href="/productDetail/ <%= product_top3[i].ID %> " ><img src="<%=product_top3[i].img1%>" alt="car" ></a>
                        <a href="/productDetail/ <%= product_top3[i].ID %> " class="rating"><i class="far fa-star"></i></a>
                        <a href="/productDetail/ <%= product_top3[i].ID %> " class="bid"><i class="flaticon-auction"></i></a>
                    </div>
                    <div class="auction-content">
                        <h6 class="title">
                            <a href="#0"><%=product_top3[i].nameProduct%></a>
                        </h6>
                        <div class="bid-area">
                            <div class="bid-amount">
                                <div class="icon">
                                    <i class="flaticon-auction"></i>
                                </div>
                                <div class="amount-content">
                                    <div class="current">Current Bid</div>
                                    <div class="amount"><%=product_top3[i].priceCurrent%></div>
                                </div>
                            </div>
                            <div class="bid-amount">
                                <div class="icon">
                                    <i class="flaticon-money"></i>
                                </div>
                                <div class="amount-content">
                                    <div class="current">Buy Now</div>
                                    <div class="amount">$<%= product_top3[i].buyNow %></div>
                                </div>
                            </div>
                        </div>
                        <div class="countdown-area">
                            <div class="countdown">
                                <div id="bid_<%=product_top3[i].ID%>counter">
                                </div>
                            </div>
                            <span class="total-bids">30 Bids</span>
                        </div>
                        <div class="text-center">
                            <a href="#0" class="custom-button">Submit a bid</a>
                        </div>
                    </div>
                </div>
            </div>
            <script type="text/javascript">
                //time down
                // Set the date we're counting down to
                tEnd[<%-JSON.stringify(product_top3[i].ID)%>] = <%- JSON.stringify(product_top3[i].timeEnd)%> ;
                counDownDate[<%-JSON.stringify(product_top3[i].ID)%>] = new Date(tEnd[ <%-JSON.stringify(product_top3[i].ID)%> ]).getTime();
                // Update the count down every 1 second
                x[<%-JSON.stringify(product_top3[i].ID)%>] = setInterval(function() {
                    // Get today's date and time
                    now[<%-JSON.stringify(product_top3[i].ID)%>] = new Date().getTime();
                    // Find the distance between now and the count down date
                    distance[<%-JSON.stringify(product_top3[i].ID)%>] = counDownDate[<%-JSON.stringify(product_top3[i].ID)%>] - now[<%-JSON.stringify(product_top3[i].ID)%>];
                    // Time calculations for days, hours, minutes and seconds
                    days[<%-JSON.stringify(product_top3[i].ID)%>] = Math.floor(distance[<%-JSON.stringify(product_top3[i].ID)%>] / (1000 * 60 * 60 * 24));
                    hours[<%-JSON.stringify(product_top3[i].ID)%>] = Math.floor((distance[<%-JSON.stringify(product_top3[i].ID)%>] % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
                    minutes[<%-JSON.stringify(product_top3[i].ID)%>] = Math.floor((distance[<%-JSON.stringify(product_top3[i].ID)%>] % (1000 * 60 * 60)) / (1000 * 60));
                    seconds[<%-JSON.stringify(product_top3[i].ID)%>] = Math.floor((distance[<%-JSON.stringify(product_top3[i].ID)%>] % (1000 * 60)) / 1000);
                    // Output the result in an element with id="demo"
                    document.getElementById("bid_<%-JSON.stringify(product_top3[i].ID)%>counter").innerHTML = days[<%-JSON.stringify(product_top3[i].ID)%>] + "d :" + hours[<%-JSON.stringify(product_top3[i].ID)%>] + "h : "
                        + minutes[<%-JSON.stringify(product_top3[i].ID)%>] + " m :" + seconds[<%-JSON.stringify(product_top3[i].ID)%>] + "s  ";
                    // If the count down is over, write some text
                    if (distance[<%-JSON.stringify(product_top3[i].ID)%>] < 0) {
                        clearInterval(x[<%-JSON.stringify(product_top3[i].ID)%>]);
                        document.getElementById("bid_<%-JSON.stringify(product_top3[i].ID)%>counter").innerHTML = "AUCTION ENDED";
                    }
                }, 1000);
                //end timedown
            </script>
            <%}%>
        </div>
    </div>
</section>
<!--============= Featured Auction Section Ends Here =============-->
<!--============= Product Auction Section Starts Here =============-->
